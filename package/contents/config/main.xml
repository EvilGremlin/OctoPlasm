<?xml version="1.0" encoding="UTF-8"?>
<kcfg xmlns="http://www.kde.org/standards/kcfg/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.kde.org/standards/kcfg/1.0 http://www.kde.org/standards/kcfg/1.0/kcfg.xsd">
    <kcfgfile name=""/>

    <group name="Connection">
        <entry name="hostname" type="String">
            <default>octoprint.local</default>
        </entry>
        <entry name="port" type="String">
            <default>5000</default>
        </entry>
        <entry name="key" type="String">
            <default></default>
        </entry>
        <entry name="pollintervalb" type="Int">
            <default>10</default>
        </entry>
        <entry name="pollintervalf" type="Int">
            <default>2</default>
        </entry>
    </group>
    
    <group name="Machine">
        <entry name="tools" type="Int">
            <default>1</default>
        </entry>
        <entry name="isheatbed" type="Bool">
            <default>true</default>
        </entry>
        <entry name="ischamber" type="Bool">
            <default>false</default>
        </entry>
    </group>
    
    <group name="Camera">
        <entry name="iscamera" type="Bool">
            <default>true</default>
        </entry>        
        <entry name="cameraurl" type="String">
            <default>http://octoprint.local:8080/?action=snapshot</default>
        </entry>
        <entry name="fps" type="Int">
            <default>10</default>
        </entry>
    </group>

    <group name="View">
        <entry name="showtleft" type="Bool">
            <default>true</default>
        </entry>
        <entry name="showpbar" type="Bool">
            <default>true</default>
        </entry>
        <entry name="panelwidth" type="Int">
            <default>250</default>
        </entry> 
        
    </group>

</kcfg>
